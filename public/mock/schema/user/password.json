{
  "code": 0,
  "msg": "success",
  "data": {
    "com": "NForm",
    "props": {
      "labelPlacement": "left",
      "labelWidth": 80
    },
    "data": {
      "oldPassword": "",
      "newPassword": "",
      "confirmPassword": ""
    },
    "children": [
      {
        "com": "NFormItem",
        "props": {
          "label": "原密码"
        },
        "children": [
          {
            "com": "NInput",
            "props": {
              "type": "password",
              "showPasswordOn": "click",
              "placeholder": "请输入原密码"
            },
            "model": "oldPassword"
          }
        ]
      },
      {
        "com": "NFormItem",
        "props": {
          "label": "新密码"
        },
        "children": [
          {
            "com": "NInput",
            "props": {
              "type": "password",
              "showPasswordOn": "click",
              "placeholder": "请输入新密码"
            },
            "model": "newPassword"
          }
        ]
      },
      {
        "com": "NFormItem",
        "props": {
          "label": "确认密码"
        },
        "children": [
          {
            "com": "NInput",
            "props": {
              "type": "password",
              "showPasswordOn": "click",
              "placeholder": "请再次输入新密码"
            },
            "model": "confirmPassword"
          }
        ]
      },
      {
        "com": "div",
        "props": {
          "class": "flex justify-end gap-12px mt-16px"
        },
        "children": [
          {
            "com": "NButton",
            "children": "取消",
            "events": {
              "click": {
                "script": "$methods.closeModal()"
              }
            }
          },
          {
            "com": "NButton",
            "props": {
              "type": "primary"
            },
            "children": "确认修改",
            "events": {
              "click": {
                "script": "$methods.submitForm({ oldPassword: state.oldPassword, newPassword: state.newPassword, confirmPassword: state.confirmPassword })"
              }
            }
          }
        ]
      }
    ]
  }
}
